<script defer>
        $('.cart').click(function () {
        $('#reload-page').val(0);
        $('#quantity-count').val(1)
        $('.quantity-display').text(1)
        $('#cartquantity').modal();
        let product_name = $(this).data('productname');
        let id = $(this).val();
        $('#cart-product-name').text(product_name);
        $('#cart-product-id').val(id);
        if ($(this).hasClass('cart-page')) {
            $('#reload-page').val(1);
        }

    });

    $('.deduction').click(function () {
        $this = $(this);
        let current_qty = parseInt($('#quantity-count').val());
        if (current_qty > 1) {
            let new_quantity = current_qty - 1;
            $this.next().text(new_quantity);
            $this.next().next().val(new_quantity);
        }
    });

    $('.addition').click(function () {
        $this = $(this);
        let current_qty = parseInt($('#quantity-count').val());
        let new_quantity = current_qty + 1;
        $this.prev().prev().text(new_quantity);
        $this.prev().val(new_quantity);
    });

    $('.confirm-cart').click(function () {
        let product_id = $('#cart-product-id').val();
        let quantity = $('#quantity-count').val();
        var token = '';
        $.ajax({
            url: 'https://24seven.com.np/cart-item-add' + '/' + product_id,
            type: 'post',
            data: {
                '_token': token,
                'qty': quantity
            },
            success: function (data) {
                $('.cart-count').attr('data-count', data[2]['count']);
                if ($('#reload-page').val() == 1) {
                    window.location.reload();
                }
            }
        });
        $('#cartquantity').modal('hide');

    });

    $('.remove').click(function () {
        var id = $(this).val();
        var token = '';
        $.ajax({
            url: 'https://24seven.com.np/cart-item-remove' + '/' + id,
            type: 'post',
            data: {
                '_token': token,
            },
            success: function (data) {
                $('.remove' + data['id']).css('display', 'none');
                $('.cart' + data['id']).css('display', 'block');

                $('#order_display' + data['id']).remove();
                $('.cart-count').attr('data-count', data['count']);
            }

        })
    });

    function showResult() {
        var detail = $('#main-search').val();
        var livesearch = $('#livesearch');
        var searchBox = $('#searchBox');
        var product = '';
        document.getElementById("livesearch").innerHTML = product;
        $.ajax({
            url: 'https://24seven.com.np/product-search-json' + '/' + detail,
            type: 'GET',
            success: function (data) {
                // console.log(data);
                searchBox.css('display', 'block');
                livesearch.css('border', '1px solid #A5ACB2');

                $.each(data['products'], function (index, value) {
                    let discount_info = "";
                    if (value['discount'] != 0 && value['discount'] != null) {
                        discount_info = '<div class="discount-info">\n' +
                            '        <span class="single-overline">Rs ' + value['product_price'] + '\n' +
                            '        </span>\n' +
                            '          <span>' + value['discount'] + '% Off</span>\n' +
                            '            </div>';
                    }
                    if (value['added_to_cart'] == 0) {
                        product = product + '   <li>' + '<a href="https://24seven.com.np/product' + '/' + value['slug'] + '"> <div class="row"><div class="col-md-8"><div class="search-span1">' + value['product_name'] + '</div></div>' +
                            ' <div class="col-md-4"><div class="search-span2 text-right"><span>RS. ' + (value['product_price'] - value['product_price'] * (value['discount'] / 100)).toFixed(2) + '</span></div>' + discount_info + ' </div></div> </a></li>';
                    } else {
                        product = product + '   <li>' + '<a href="https://24seven.com.np/product' + '/' + value['slug'] + '"> <div class="row"><div class="col-md-8"><div class="search-span1">' + value['product_name'] + '</div></div>' +
                            ' <div class="col-md-4"><div class="search-span2 text-right"><span>RS. ' + (value['product_price'] - value['product_price'] * (value['discount'] / 100)).toFixed(2) + '</span></div>' + discount_info + ' </div></div> </a></li>';

                    }

                    // console.log(product);
                });

                $.each(data['generic'], function (index, value) {
                    //product = product + '<a href="https://24seven.com.np/product' + '/' + value['slug'] + '"><div>' + value['product_name'] + '</div></a>';
                    product = product + '   <li>' + '<a href="https://24seven.com.np/product' + '/medicine-brand/' + index + '"> <div class="row"><div class="col-md-9"><div class="search-span1">' + index + '</div></div>' +
                        ' <div class="col-md-3"><div class="search-span2 text-right">' + value + ' Variants</div> </div></div> </a></li>';
                    // console.log(product);
                });


                $.each(data['name'], function (index, value) {
                    product = product + '   <li>' + '<a href="https://24seven.com.np/generic-name' + '/' + value['name'] + '"> <div class="row"><div class="col-md-8"><div class="search-span1">' + value['name'] + '</div></div>' +
                        '</a></li>';

                    // console.log(product);
                });

                document.getElementById("livesearch").innerHTML = product;
            }
        });
        if (detail == null) {
            searchBox.css('display', 'none');
        }

    }

    $('#main-search').focusout(function () {
        var searchBox = $('#searchBox');
        setTimeout(function () {
            searchBox.css('display', 'none');
        }, 300);
    });
    $('#main-search').focusin(function () {
        showResult();
    });

    $('.dropdown-item').click(function () {
        $.ajax({
            url: 'https://24seven.com.np/get-notification',
            type: 'POST',
            data: {
                '_token': '',
                'id': $(this).data('id')
            },
            success: function (data) {
                if (data != 'Error') {
                    console.log(data)
                    $('#notification-text').text(data);
                }
            }
        });
        $('#notification').modal('show');
    })

</script>

<script defer>
    function validateEmail(email) {
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return re.test(email);
    }

    $(document).ready(function () {
        $("#submite").click(function () {
            var email = $("#oneemail").val();
            if (email === "") {
                $('#alertmessage').css('display', 'inline-block');
                $('#alertmessage').text('Empty Email').fadeOut(4000);
            } else {
                var valid_email = validateEmail(email);
                if (valid_email === false) {
                    $('#alertmessage').css('display', 'inline-block');
                    $('#alertmessage').text('Invalid Email').fadeOut(4000);
                }
                if (valid_email === true) {
                    var token = '';
                    $.ajax({
                        type: 'post',
                        url: 'https://24seven.com.np/emailsubscribe',
                        data: {
                            '_token': token,
                            'email': email
                        },
                        success: function (data) {
                            $('.subscribe-input').css('display', 'none');
                            $('.subscription-result').css('display', 'block');
                            $('.subscription-result').text(data[1]);
                        }
                    })
                }
            }

        });
    });

    $("div#general_prescription_images").dropzone({
        url: "https://24seven.com.np/general-dropzone-prescription",
        params: {
            '_token': ''
        },
        maxFiles: 10,
        addRemoveLinks: true,
        init: function () {
            this.on("success", function (file, response) {
                $('.dz-remove').attr('data-input', response);
                $('#general_prescription').append('<input type="hidden" id="prescriptions" name="prescriptions[]" multiple  value="' + response + '">')
            })
        },
        removedfile: function (file) {
            filevalue = file._removeLink.dataset.input;
            $('input[value="' + filevalue + '"]').remove();


            var _ref;
            return (_ref = file.previewElement) != null ? _ref.parentNode.removeChild(file.previewElement) : void 0;
        }

    });
</script>


